<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="allhabiy.sda">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegistrationActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".DonatorLoginActivity"></activity>
        <activity android:name=".NeedyLoginActivity"></activity>
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".AboutSDAActivity"></activity>
        <activity android:name=".AboutDevelopersActivity"></activity>

    </application>

</manifest>
